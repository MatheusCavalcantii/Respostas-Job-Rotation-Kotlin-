fun main(args: Array<String>) {

    val numInput = 13
    var num1 = 0
    var num2 = 1

    println("O número é: $numInput")

    for (i in 1..numInput) {
        println("$num1 ")

        val sum = num1 + num2
        num1 = num2
        num2 = sum
        if (num1 == numInput) {
            println("[$numInput] está dentro  da sequência de Fibonnace!")
        }
    }
}
